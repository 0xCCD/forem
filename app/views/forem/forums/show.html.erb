<%= render :partial => 'forem/forums/head', :locals => { :forum => @forum } %>

<%= paginate @topics %>
<table class='topics'>
  <thead>
    <tr>
      <th></th>
      <th>Topic</th>
      <th>Latest</th>
      <th>Posts</th>
      <th>Views</th>
    </tr>
  </thead>
  <tbody>
    <%= render :partial => 'forem/topics/topic_listing', :collection => @topics %>
  </tbody>
</table>
